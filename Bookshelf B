#include<bits/stdc++.h>
using namespace std;
int a[20005];
bool cmp(int a,int b)
{
    return a>b;
}
int main()
{
    int n , B,sum = 0, cnt = 0;
	cin>>n>>B;
	for(int i = 0;i < n;++i)
    {
        cin>>a[i];
    }
    sort(a ,a + n,cmp );
   for(int i = 0;i < n;++i)
    {
        if(sum < B)
        {
            sum += a[i];
            ++cnt;
        }
        else
        break;
    }
    cout<<cnt<<endl;
}
