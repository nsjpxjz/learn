#include<bits/stdc++.h>
using namespace std;
int a[32][32];
int b[32][32];
int dx[4]={0,0,1,-1};
int dy[4]={-1,1,0,0};
int n;
void dfs(int x,int y)
{
    if(x < 0||x > n+1||y < 0||y > n+1||b[x][y]!=0)
    return;
    b[x][y] = 1;
    for(int i=0;i<4;++i)
        {
            dfs(x+dx[i],y+dy[i]);

        }
}
int main()
{
    cin>>n;
    for(int i=1;i<=n;i++)
        for(int j=1;j<=n;j++)
    {
        cin>>a[i][j];
        if(a[i][j] == 1)
        b[i][j] = 1;
        else if(a[i][j] == 0)
            b[i][j] = 0;
    }

    dfs(0,0);
    for(int i = 1;i<=n;++i)
     {
       for(int j = 1;j <= n;++j)

        if (b[i][j]==0)
            cout<<2<<' ';
        else cout <<a[i][j]<<' ';
        cout<<'\n';
    }
    return 0;
}
