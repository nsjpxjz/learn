#include<bits/stdc++.h>
using namespace std;
int a[100005];
const int M=1000000007;
int n,x,c[5003],u = 0,ans=0,ch;
int main()
{
    cin>>n;
    for(int i=1;i<=n;++i){
        cin>>x;
        ++c[x];
        if(u<x) u = x;
    }
    for(int i=1;i<=u;++i){
        for(int j=0;j*2<=i;++j){
        	if(2*j==i) ch=c[j]*(c[j]-1)/2;
        	else ch=c[j]*c[i-j];
            ans=(ans+ch*c[i]*(c[i]-1)/2)%M;
        }
    }
    cout<<ans<<endl;
return 0;
}
