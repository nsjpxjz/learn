#include<bits/stdc++.h>

using namespace std;
int a[23];
bool judge(int n)
{
    for(int i = 2;i * i <= n;++i)
    {
        if(n % i == 0)
        return 0;
    }
    return 1;
}
int found(int left_number,int current_sum,int start,int last)
{
    if(left_number == 0) return judge(current_sum);
    int sum = 0;
    for(int i = start;i <= last;++i)
        sum += found(left_number - 1,current_sum + a[i],i+1,last);
    return sum;
}


int main ()
{
  int n,k;
  cin>>n>>k;
  for(int i = 1;i <= n;++i)
    cin>>a[i];
  cout<<found(k,0,1,n);
}
