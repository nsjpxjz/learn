#include<bits/stdc++.h>
using namespace std;
int a[10005],n;

int main()
{
    int i,j,tmp,cnt = 0;
    cin>>n;
    for(i = 0;i < n;++i)
    {
        cin>>a[i];
    }
    for(i = 0;i < n - 1;++i)
    {
        for(j = 0;j < n - 1;++j)
        {
            if(a[j] > a[j+1])
            {
                tmp = a[j];
                a[j] = a[j+1];
                a[j+1] = tmp;
                ++cnt;
            }
        }
    }
    cout<<cnt;
}
