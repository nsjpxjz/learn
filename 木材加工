#include<bits/stdc++.h>
using namespace std;
int k,n,a[100000];

bool judge(int x,int k)
{
    int tot = 0;
    for(int i = 0;i < n;++i)
    {
        tot += a[i]/x;
    }
    return tot >= k;
}
int main()
{
    cin>>n>>k;
   for(int i = 0;i <n;++i)
    cin>>a[i];
    int l = 1,r = 100000000;
    while(l < r)
    {
        int mid = l + r>>1;
      if(judge(mid,k))
      {
          l = mid + 1;
      }

      else
        r = mid;

    }
    cout<<l -1;

}
